{
	"name": "AutoDJ Extension",
	"version": "1.0",
	"description": "Generate playlists",
	"background": {
		"scripts": ["jquery-3.3.1.min.js","background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"manifest_version": 2,
	"permissions": [
		"https://*/",
		"http://*/"
	]
}